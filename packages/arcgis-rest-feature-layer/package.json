{
  "name": "@esri/arcgis-rest-feature-layer",
  "version": "3.3.0",
  "description": "Feature layer query and edit helpers for @esri/arcgis-rest-js",
  "license": "Apache-2.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/umd/feature-layer.umd.min.js",
  "types": "dist/esm/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=12.20.0"
  },  "files": [
    "dist/**"
  ],
  "dependencies": {
    "@esri/arcgis-rest-types": "^3.3.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@esri/arcgis-rest-request": "^3.3.0"
  },
  "peerDependencies": {
    "@esri/arcgis-rest-request": "^3.0.0"
  },
  "scripts": {
    "prebuild": "tsc --noEmit && trash ./dist",
    "build": "npm run build:cjs && npm run build:esm && npm run build:umd",
    "postbuild": "node ../../scripts/create-dist-package-jsons.js",
    "build:esm": "tsc --outDir ./dist/esm --declaration",
    "build:cjs": "tsc --outDir ./dist/cjs -m commonjs",
    "build:umd": "rollup -c ../../umd-base-profile.js && rollup -c ../../umd-production-profile.js",
    "dev:esm": "tsc -w --outDir ./dist/esm --declaration",
    "dev:cjs": "tsc -w --outDir ./dist/cjs -m commonjs",
    "dev:umd": "rollup -w -c ../../umd-base-profile.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Esri/arcgis-rest-js.git",
    "directory": "packages/arcgis-rest-feature-layer"
  },
  "contributors": [
    {
      "name": "Tom Wayson",
      "email": "twayson@esri.com",
      "url": "http://tomwayson.com/"
    }
  ],
  "bugs": {
    "url": "https://github.com/Esri/arcgis-rest-js/issues"
  },
  "homepage": "https://github.com/Esri/arcgis-rest-js#readme",
  "keywords": [
    "typescript",
    "promise",
    "fetch",
    "arcgis",
    "esri",
    "ES6"
  ]
}