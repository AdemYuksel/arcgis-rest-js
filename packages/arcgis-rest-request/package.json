{
  "name": "@esri/arcgis-rest-request",
  "version": "3.3.0",
  "description": "Common methods and utilities for @esri/arcgis-rest-js packages.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "unpkg": "dist/request.umd.min.js",
  "sideEffects": false,
  "engines": {
    "node": ">=12.0.0"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@esri/arcgis-rest-fetch": "3.3.0",
    "formdata-node": "^4.0.1",
    "tslib": "^2.3.1"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "prebuild": "tsc --noEmit",
    "build": "npm run build:node && npm run build:esm && npm run build:umd",
    "postbuild": "../../support/create-dist-package-jsons.js && npx package-check",

    "build:esm": "tsc --outDir ./dist/esm --declaration",
    "build:node": "tsc --outDir ./dist/cjs -m commonjs",
    "build:umd": "rollup -c ../../umd-base-profile.js && rollup -c ../../umd-production-profile.js",
    "build:cdn": "rollup -c ../../esm-cdn-profile.js",
    "dev:esm": "tsc -w --outDir ./dist --declaration",
    "dev:umd": "rollup -w -c ../../umd-base-profile.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Esri/arcgis-rest-js.git",
    "directory": "packages/arcgis-rest-request"
  },
  "contributors": [
    {
      "name": "Patrick Arlt",
      "email": "parlt@esri.com",
      "url": "http://patrickarlt.com/"
    }
  ],
  "bugs": {
    "url": "https://github.com/Esri/arcgis-rest-js/issues"
  },
  "homepage": "https://github.com/Esri/arcgis-rest-js#readme",
  "keywords": [
    "typescript",
    "promise",
    "fetch",
    "arcgis",
    "esri",
    "ES6"
  ],
  "esri": {
    "keyExports": [
      "request"
    ]
  }
}
