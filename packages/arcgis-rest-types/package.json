{
  "name": "@esri/arcgis-rest-types",
  "version": "3.3.0",
  "description": "Shared TypeScript types for @esri/arcgis-rest-js",
  "license": "Apache-2.0",
  "keywords": [
    "ES6",
    "arcgis",
    "esri",
    "fetch",
    "promise",
    "typescript"
  ],
  "main": "src/index.ts",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/types/*.d.ts",
    "src/**/*.ts"
  ],
  "scripts": {
    "build": "npm run build:esm",
    "build:esm": "tsc --emitDeclarationOnly",
    "dev:esm": "tsc -w --emitDeclarationOnly"
  },
  "author": "",
  "contributors": [
    "Jeff Jacobson (http://github.com/jeffjacobson)",
    "Patrick Arlt <parlt@esri.com> (http://patrickarlt.com/)"
  ],
  "homepage": "https://github.com/Esri/arcgis-rest-js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Esri/arcgis-rest-js.git",
    "directory": "packages/arcgis-rest-types"
  },
  "bugs": {
    "url": "https://github.com/Esri/arcgis-rest-js/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "esri": {
    "keyExports": [
      "IUser, IItem"
    ]
  }
}
